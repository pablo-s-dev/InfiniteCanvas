{
  "expo": {
		"name": "InfiniteCanvas",
		"slug": "InfiniteCanvas",
		"version": "1.0.1",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
      "versionCode": 2,
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.PabloS.InfiniteCanvas"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"extra": {
			"eas": {
				"projectId": "025bcd27-795c-4aa9-843a-94ddddd4c675"
			}
		},
		"plugins": [["./plugins/disableForcedDarkModeAndroid.js", {}]]
	}
}
